{
  "name": "RTL_433 US",
  "version": "0.4.06",
  "slug": "rtl433-us",
  "description": "Integrates rtl_433 with Home Assistant using an RTL-SDR dongle. Allows the user to select either 433MHz or 915MHz frequency bands. Writes to MQTT broker.",
  "arch": ["aarch64", "armv7", "armhf", "amd64"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "full_access": true,
  "devices": [
    "/dev/bus/usb:/dev/bus/usb:rw",
    "/dev/bus/usb/001/003:/dev/bus/usb/001/003"
  ],
  "options": {
    "mqtt_host": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_user": "",
    "mqtt_pass": "",
    "frequency": 433
  },
  "schema": {
    "mqtt_host": "str?",
    "mqtt_port": "int?",
    "mqtt_user": "str?",
    "mqtt_pass": "str?",
    "frequency": "int(433|915)?"
  }
}
