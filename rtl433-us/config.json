{
  "name": "RTL_433 US",
  "version": "1.0.10",
  "slug": "rtl433-us",
  "description": "Integrates rtl_433 with Home Assistant using RTL-SDR dongles (supports multiple with per-dongle frequencies).",
  "arch": ["aarch64", "armv7", "armhf", "amd64"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "full_access": true,
  "devices": [
    "/dev/bus/usb:/dev/bus/usb:rw"
  ],
  "options": {
    "mqtt_host": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_user": "",
    "mqtt_password": "",
    "dongles": [
      {
        "device": "00000433",
        "frequency": 433
      },
      {
        "device": "00000915",
        "frequency": 915
      }
    ]
  },
  "schema": {
    "mqtt_host": "str?",
    "mqtt_port": "int?",
    "mqtt_user": "str?",
    "mqtt_password": "password?",
    "dongles": [
      {
        "device": "str",
        "frequency": "int(400,1000)?"
      }
    ]
  }
}